{% for image in field.value %}                 
<div class="d-flex align-items-center">
  <div class="flex-shrink-0">
    <a href="#" class="ea-lightbox-thumbnail" data-ea-lightbox-content-selector="#ea-lightbox-{{image.id}}">
        <img alt="{{image.image}}" class="img-thumbnail" src="/uploads/images/chantier/{{image.image}}" height="200" width="200">
    </a>

    <div id="ea-lightbox-{{image.id}}" class="ea-lightbox">
        <img alt="{{image.image}}" src="/uploads/images/chantier/{{image.image}}">
    </div>
  </div>
  <div class="flex-grow-1 ms-3">
    Titre: {{image.titre}}
</div>
</div>
{% endfor %}